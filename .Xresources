! special
*.foreground:   #ffffff
*.background:   #0c0c0c
*.cursorColor:  #ffffff

! black
*.color0:       #2a1d17
*.color8:       #4f362b

! red
*.color1:       #da1657
*.color9:       #da4375

! green
*.color2:       #3ea250
*.color10:      #6cb87a

! yellow
*.color3:       #e3d33d
*.color11:      #e3da84

! blue
*.color4:       #3ea290
*.color12:      #8ca8a3

! magenta
*.color5:       #ff850d
*.color13:      #ffa64f

! cyan
*.color6:       #8c16da
*.color14:      #a167c7

! white
*.color7:       #e9e9e9
*.color15:      #fdfdfd


!font
URxvt.font: xft:Consolas:size=12,\
	    xft:Kochi Gothic:antialias=false,\
	    xft:Baekmuk Gulim
URxvt.scrollBar: false
URxvt.letterSpace: 0

!Extensions:
URxvt.perl-ext-common: default,url-select,resize-font
URxvt.perl-ext: default,url-select,resize-font
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true
URxvt.resize-font.smaller: C-j
URxvt.resize-font.bigger: C-k
URxvt.resize-font.reset: C-h
